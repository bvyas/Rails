<h1>Listing Users</h1>
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>Category</th>
	</tr>	
	<%@users.each do |user| %>
	<tr>
	  <td><%= user.first_name%></td>
	  <td><%= user.last_name%></td>
	  <td><%= user.email_address%></td>
	  <td><%= user.category_id%></td>
	 <td> | <a href="/user/show/<%= user.id %>">show</a> | </td> 
	 <td><a href="/user/edit/<%= user.id %>">edit</a> | </td> 
	<!--  <td><a href="/user/destroy">destroy</a></td> -->
	<td> <a href= "/user/destroy/<%= user.id %>" data-method='delete'>delete</a></td>
	</tr> 
	<%end%>

</table>
<div class = first>
	<a href="/user/new"><button>New</button></a>
</div>		



<p>Find me in app/views/user/index.html.erb</p>
